{
  "name": "auto-shutdown",
  "version": "1.0.0",
  "description": "Small Chrome extension project for automatic computer shutdown",
  "main": "index.js",
  "scripts": {
    "build-popup": "cross-env ENV=dev webpack --config ./popup/webpack.config.js --mode production",
    "build-popup-prod": "webpack --config ./popup/webpack.config.js --mode production",
    "build-content-script": "cross-env ENV=dev webpack --config ./content-script/webpack.config.js --mode production",
    "build-content-script-prod": "webpack --config ./content-script/webpack.config.js --mode production",
    "build-background": "cross-env ENV=dev webpack --config ./background/webpack.config.js --mode production",
    "build-background-prod": "webpack --config ./background/webpack.config.js --mode production",
    "build": "npm run build-popup && npm run build-content-script && npm run build-background",
    "build-prod": "npm run build-popup-prod && npm run build-content-script-prod && npm run build-background-prod",
    "test": "jest"
  },
  "browser": {
    "child_process": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senaa12/shutdown-extension.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Redux",
    "Typescript",
    "Sass",
    "Javascript",
    "Chrome",
    "Jest"
  ],
  "author": "Luka Senicic",
  "license": "MIT",
  "dependencies": {
    "@types/react-transition-group": "^4.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^10.4.9",
    "@types/chrome": "0.0.91",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.6.0",
    "deepmerge": "^4.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^29.5.0",
    "jest-webextension-mock": "^3.6.1",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "react-particles-js": "^3.4.0",
    "react-redux": "^7.2.1",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2",
    "source-map": "^0.7.3",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^4.3.0",
    "ts-jest": "^26.3.0",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7",
    "typescript-tslint-plugin": "^0.5.5",
    "webext-redux": "^2.1.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
