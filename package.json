{
  "name": "auto-shutdown",
  "version": "1.0.0",
  "description": "Small Chrome extension project for automatic computer shutdown",
  "main": "index.js",
  "scripts": {
    "build-popup": "cross-env ENV=dev webpack --config ./popup/webpack.config.js --mode production",
    "build-popup-prod": "webpack --config ./popup/webpack.config.js --mode production",
    "build-content-script": "cross-env ENV=dev webpack --config ./content-script/webpack.config.js --mode production",
    "build-content-script-prod": "webpack --config ./content-script/webpack.config.js --mode production",
    "build-background": "cross-env ENV=dev webpack --config ./background/webpack.config.js --mode production",
    "build-background-prod": "webpack --config ./background/webpack.config.js --mode production",
    "build": "npm run build-popup && npm run build-content-script && npm run build-background",
    "build-prod": "npm run build-popup-prod && npm run build-content-script-prod && npm run build-background-prod",
    "test": "jest"
  },
  "browser": {
    "child_process": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senaa12/shutdown-extension.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Redux",
    "Typescript",
    "Sass",
    "Javascript",
    "Chrome",
    "Jest"
  ],
  "author": "Luka Senicic",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^14.1.0",
    "@types/chrome": "^0.0.250",
    "@types/jest": "^25.2.3",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.9",
    "@types/react-transition-group": "^4.4.9",
    "babel-loader": "^9.1.3",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "deepmerge": "^4.2.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^25.5.4",
    "jest-webextension-mock": "^3.6.1",
    "react-redux": "^7.2.1",
    "react-transition-group": "^4.4.1",
    "react-tsparticles": "^2.12.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^3.3.3",
    "svg-sprite-loader": "^6.0.11",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.3.0",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^6.1.3",
    "tsparticles": "^2.12.0",
    "typescript": "^4.9.5",
    "typescript-tslint-plugin": "^0.5.5",
    "webext-redux": "^2.1.7",
    "webpack": "^5.72.0",
    "webpack-cli": "^5.1.4"
  }
}
