{
    "extends": "tslint:recommended",
    "rules": {
        "no-trailing-whitespace": [ true, "always" ],
        "prefer-const": [ true ],
        "semicolon": [ true, "always" ],
        "space-before-function-paren": [ true, "never" ],
        "quotemark": [ true, "single" ],
        "no-console": true
    }
}